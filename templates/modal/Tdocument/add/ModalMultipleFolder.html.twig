

<!-- Modal -->
<div class="modal fade" id="exampleMultipleModalFolder" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <form  onsubmit="return false" id="folder" name="tdocument" action="{{ path('tdocument_folder_new',{'code':code_niveau}) }}" method="post" enctype="multipart/form-data">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Ajouter un dossier </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

                <div class="alert alert-success success-add mb-2" role="alert">
                    <i class="fas fa-check"></i> Bien ajouté
                </div>

                <div class="form-group  mt-3">
                        <input type="text" name="tdocument[intitule]" class="form-control border-info" id="ajout_intitule" placeholder="Saisir l'intitulé du dossier">
                </div>
                <div class="text-danger intitule error-custom mt-2"  role="alert">
                     <i class="fas fa-exclamation-triangle"></i>
                </div>
                
                <input type="hidden" id="tdocument__token" name="tdocument[_token]" value="{{ csrf_token('tdocument') }}">
            <button type="button" class="btn btn-info btn-sm  button-size ajouter_multi_dossier">Ajouter</button>
            </div>
        <p id="demo"  style=" padding:3px;"></p>
            <div class="modal-footer">

                <button type="submit" class="btn btn-info btn-sm  button-size televerser " id='multi-televerser' disabled >Téléverser</button>
                <div class="loader"></div>
                <button type="button" class="btn btn-danger  btn-sm  button-size fermer-modal" data-dismiss="modal">Fermer</button>

            </div>
        </div>
    </form>

  </div>
</div>
<!-- modal -->